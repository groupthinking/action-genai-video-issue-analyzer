# Video Action Analyzer - Environment Variables
# Copy this file to .env and fill in your values

# Required: Google API Key for Gemini analysis and transcription
GOOGLE_API_KEY=your_google_api_key_here

# Optional: OpenAI API Key for Whisper transcription (fallback to Gemini if not set)
OPENAI_API_KEY=your_openai_api_key_here

# Cloudflare Deployment (set via wrangler secret put)
# CLOUDFLARE_API_TOKEN=your_cloudflare_token
# CLOUDFLARE_ACCOUNT_ID=your_account_id

# GitHub (for resolving asset URLs from issues)
GITHUB_TOKEN=your_github_token

# Environment
NODE_ENV=development
